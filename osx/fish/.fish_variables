# add /usr/local/sbin to your PATH Variable
set PATH $PATH /usr/local/bin

# postgresql@9.5
set -g fish_user_paths '/usr/local/opt/postgresql@9.5/bin' $fish_user_paths

# npm path
set -gx PATH $PATH '/Users/esnilara/.asdf/installs/nodejs/8.9.4/.npm/bin'

# make vim the default editor
set --export EDITOR 'vim -f'

# make vim usable with git
set --export GIT_EDITOR 'vim -f'

# set ag as default for fzf
set -g -x FZF_DEFAULT_COMMAND 'ag -g ""'
